{
  "expo": {
    "name": "iNery",
    "slug": "iNery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "linear-gradient(130.02deg, #FF9900 47.43%, #FFCC00 81.83%)"
    },
    "updates": {
